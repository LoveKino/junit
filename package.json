  "description": "A simple promise based module for unit tests.",
  "main": "lib/index.js",
  "scripts": {
    "test": "no test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ysmood/ken.git"
  },
  "keywords": [
    "unit",
    "test",
    "promise",
    "async",
    "sync"
  ],
  "author": "http://ysmood.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ysmood/ken/issues"
  },
  "homepage": "https://github.com/ysmood/ken#readme",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "babel": "^5.8.23",
    "browserify": "^11.1.0",
    "diff": "^2.1.1",
    "nokit": "^0.13.0",
    "yaku": "^0.7.4"
  },
  "eslintConfig": {
